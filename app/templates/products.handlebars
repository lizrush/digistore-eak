<div class='row'>
  <div class='col-lg-2'>
    <h4>More Cats:</h4>
    <ul class='list-unstyled'>
      {{# each }}
        <li>
          {{# link-to "product" this }}
            <img {{ bind-attr src=avatar }} />
            {{ name }}
          {{/ link-to }}
        </li><br>
      {{/ each }}
    </ul>
  </div>
  <div class='col-lg-10'>
    {{ outlet }}
  </div>
</div>
